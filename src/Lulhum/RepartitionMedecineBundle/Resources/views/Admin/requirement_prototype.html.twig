<div class="panel panel-default">
	<div class="panel-heading" >
		<label class="required">__name__label__</label>
	</div>
	<div class="panel-body" >
		<div class="form-group" >
			{{ form_label(form.type, null, {'label_attr': {'class': 'control-label col-md-2'}}) }}
			{{ form_errors(form.type) }}
			<div class="col-md-10" >
				{{ form_widget(form.type) }}
			</div>
		</div>
		<div class="form-group" >
			{{ form_label(form.params, null, {'label_attr': {'class': 'control-label col-md-2'}}) }}
			{{ form_errors(form.params) }}
			<div class="col-md-10" >
				{{ form_widget(form.params) }}
			</div>
		</div>
		<div class="form-group" >
			{{ form_label(form.strict, null, {'label_attr': {'class': 'control-label col-md-2'}}) }}
			{{ form_errors(form.strict) }}
			<div class="col-md-10" >
				{{ form_widget(form.strict, {'attr': {'style': 'width:0'}}) }}
			</div>
		</div>
	</div>
	<div class="panel-footer" ></div>
</div>
